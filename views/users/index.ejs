<%- include('../partials/boilerplate') %>
<body>
    <%- include('../partials/header') %>
    <h1>DavidBook - Users</h1>
    <% for (let i = 0; i < users.length; i++) { %>
        <b><p><a href=<%= '/users/profile/' + users[i].uuid %>><%= users[i].username %></a></p></b>
        <img src=<%= users[i].img %> alt="User"/>
        
        <p><%= users[i].aboutMe %></p>
    <% } %>
</body>
</html>